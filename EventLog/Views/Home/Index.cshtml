@{
    Layout = null;
}
<!DOCTYPE html>

<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Warehouse 13</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.css" rel="stylesheet" type="text/css">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div>
        <nav class="navbar navbar-brown" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-1">
                    <i class="fa fa-bars sr-only">Toggle navigation</i>
                </button>
                <a class="navbar-brand" href="#">Warehouse 13</a>
            </div>
            <div class="collapse navbar-collapse" id="navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a class="dropdown-toggle bg-brown-invert img-circle" data-toggle="dropdown">
                            <i class="fa fa-user fa-lg" title="User Info"></i><b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu pull-right">
                            <li>
                                <a href="#/pages/profile">
                                    <i class="fa fa-user"></i>
                                    <span data-i18n="My Profile">My Profile</span>
                                </a>
                            </li>
                            <li>
                                <a href="#/pages/logout">
                                    <i class="fa fa-sign-out"></i>
                                    <span data-i18n="Log Out">Log Out</span>
                                </a>
                            </li>
                            <li>
                                <a href="#/pages/login">
                                    <i class="fa fa-sign-in"></i>
                                    <span data-i18n="Log In">Log In</span>
                                </a>
                            </li>

                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
        <aside id="leftnav">
            <ul class="list-group">
                <li class="list-group-item active">
                    <a href="#">
                        <i class="fa fa-leaf">
                            <span class="icon-bg bg-blue"></span>
                        </i>
                        <span>Simple Repository</span>
                    </a>
                </li>
                <li class="list-group-item">
                    <a href="#">
                        <i class="fa fa-magic">
                            <span class="icon-bg bg-red"></span>
                        </i>
                        <span>MongoRepository</span>
                    </a>
                </li>
            </ul>
        </aside>
        <main id="content">

            <!-- Keep all page content within the page-content inset div! -->
            <div class="page-content container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <p class="lead">This simple sidebar template has a hint of JavaScript to make the template responsive. It also includes Font Awesome icon fonts.</p>
                    </div>
                    <div class="col-md-6">
                        <p class="well">The template still uses the default Bootstrap rows and columns.</p>
                    </div>
                    <div class="col-md-6">
                        <p class="well">But the full-width layout means that you wont be using containers.</p>
                    </div>
                    <div class="col-md-4">
                        <p class="well">Three Column Example</p>
                    </div>
                    <div class="col-md-4">
                        <p class="well">Three Column Example</p>
                    </div>
                    <div class="col-md-4">
                        <p class="well">You get the idea! Do whatever you want in the page content area!</p>
                    </div>
                </div>

            </div>

        </main>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

</body>
</html>
